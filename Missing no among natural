//Missing among natural number example 1,2,3,5,6
//missing is 4
package com.practice.coding;

public class MissingNumber {
	
	public void printArray(int arr[])
	{
		int n=arr.length;
		for(int i=0;i<n;i++)
		{
			System.out.print(arr[i]+" ");
		}
		System.out.println();
	}
	
	public int missingArray(int arr[])
	{
		int n=arr.length+1;
		int sum=n*(n+1)/2;
		
		for(int num:arr)
		{
			sum=sum-num;
		}
		return sum;
	}
	
	public static void main(String arg[])
	{
		MissingNumber m=new MissingNumber();
		int arr[]= {1,3,5,4,6};
		m.printArray(arr);
		System.out.println(m.missingArray(arr));
		
	}
	

}
