public class MissingNumber {
	
	 
	  
	    static int getMissingNo(int a[], int n)
	    {
	        int count = 1;
	        for (int i = 2; i <= (n + 1); i++)
	        {
	            count=count+ i;
	            count =count- a[i - 2];
	        }
	        return count;
	    }
	 
	   
	    public static void main(String[] args)
	    {
	        int[] arr = { 1, 2, 3, 5 };
	        System.out.println(getMissingNo(arr, arr.length));
	    }
	

}
