
public class Assert {
	


	    public static String assertEquals(String input,String string)
	    {     int a = input.length();int h;
	        String result = "";
	        char[] ch = input.toCharArray();
	        for(int i =0;i<a;i++) {
	            h = 1;
	            while (i < a - 1 && ch[i] == ch[i + 1]) {

	                h = h + 1;
	                i++;

	            }
	            result = result + ch[i] + h;

	        }
	        return result;
	    }
	    public static void doTestPass()
	    {
	    	
	        Assert.assertEquals("",assertEquals("",null));
	        Assert.assertEquals("a1",assertEquals("a",null));
	        Assert.assertEquals("a3",assertEquals("aaa",null));
	        Assert.assertEquals("a3b3a2d1",assertEquals("aaabbbaad",null));
	    }
	    public static void main() throws InterruptedException
	    {
	        doTestPass();
	    }
	}
