
public class Frequency {
	

 static int countOccurrences(int arr[],  int x)
 {
     int res = 0;
     for (int i=0; i<arr.length; i++)
         if (x == arr[i])
           res++;
     return res;
 }
  
 public static void main(String args[])
 {
     int arr[] = {1, 2, 2, 2, 2, 2,3};
     int n = arr.length;
     int x = 2;
     System.out.println(countOccurrences(arr,x));
 }
}
